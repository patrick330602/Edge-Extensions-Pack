{
  "name": "FlyingYouTube",
  "version": "0.4",
  "manifest_version": 2,
  "author": "Denaxin Software",
  "description": "Download Youtube videos",
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "js/flyingyoutube.js" ],
      "matches": [ "*://*.youtube.com/*" ],
      "run_at": "document_end"
   } ],
   "icons": {
      "16": "icons/icon_16.png",
      "32": "icons/icon_32.png",
      "64": "icons/icon_64.png",
      "128": "icons/icon_128.png"
   }
}