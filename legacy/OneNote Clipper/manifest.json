{
	"name": "OneNote Clipper",
	"version": "2.1.4.2",
	"background": {
		"page": "EdgeExtension.html",
		"persistent": true
	},
	"description": "__MSG_appDesc__",
	"default_locale": "en",
	"author": "Microsoft Corporation",
	"permissions": [
		"<all_urls>",
		"activeTab",
